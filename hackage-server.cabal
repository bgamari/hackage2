name: hackage-server
version: 0.1
build-type: Simple
cabal-version: >=1.2

executable hackage-server
  main-is: Main.hs
  build-depends:
    base, Cabal >= 1.3.11 && < 1.5,
    HAppS-Data   >= 0.9.2 && < 1.0,
    HAppS-IxSet  >= 0.9.2 && < 1.0,
    HAppS-State  >= 0.9.2 && < 1.0,
    HAppS-Server >= 0.9.2 && < 1.0
